<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">
	<persistence-unit name="JPAManager" transaction-type="JTA">
		<jta-data-source>jdbc/SherpaDS</jta-data-source>
		<class>prueba.jpa.model.Cliente</class>
		<class>prueba.jpa.model.TarjetaVirtual</class>
		<class>prueba.jpa.model.DivisionGeografica</class>
		<class>prueba.jpa.model.Parametro</class>
		<class>prueba.jpa.model.TipoParametro</class>
		<shared-cache-mode>ENABLE_SELECTIVE</shared-cache-mode>
	</persistence-unit>
</persistence>
